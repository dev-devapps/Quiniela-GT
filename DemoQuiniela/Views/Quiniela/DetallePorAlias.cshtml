@model MvcQuiniela.ViewModel.QuinielaViewModel
@{
    TempData["DatosLogin"] = ViewBag.DatosLogin;

    ViewBag.Title = "Detalle de Puntos Por Alias";
    Layout = "~/Views/Shared/_LayoutQuiniela.cshtml";


}
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<!-- Basic Examples -->
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    Detalle de Puntos De @Model.vm_pronosticos.ElementAt(0).alias
                </h2>
            </div>
            <div class="body">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                        <thead>
                            <tr>
                                <th>Equipo</th>
                                <th>Marcador</th>
                                <th>Pronostico</th>
                                <th>Pronostico</th>
                                <th>Marcador</th>
                                <th>Equipo</th>
                                <th>Puntos</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0; i < Model.vm_pronosticos.Count(); i++)
                            {
                                <tr>
                                    <td>@Model.vm_pronosticos.ElementAt(i).equipo1</td>
                                    <td>@Model.vm_pronosticos.ElementAt(i).marcador1</td>
                                    <td>@Model.vm_pronosticos.ElementAt(i).pronostico1</td>
                                    <td>@Model.vm_pronosticos.ElementAt(i).pronostico2</td>
                                    <td>@Model.vm_pronosticos.ElementAt(i).marcador2</td>
                                    <td>@Model.vm_pronosticos.ElementAt(i).equipo2</td>
                                    <td>@Model.vm_pronosticos.ElementAt(i).puntos</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- #END# Basic Examples -->
